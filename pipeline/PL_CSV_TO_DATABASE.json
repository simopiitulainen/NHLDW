{
	"name": "PL_CSV_TO_DATABASE",
	"properties": {
		"activities": [
			{
				"name": "Teams to SQL",
				"description": "Copies team data from JSON to SQL staging area",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Truncate staging tables",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "teams",
							"wildcardFileName": "part*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "teams_id",
									"type": "String"
								},
								"sink": {
									"name": "id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "teams_active",
									"type": "String"
								},
								"sink": {
									"name": "link",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_firstYearOfPlay",
									"type": "String"
								},
								"sink": {
									"name": "venue_name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_franchiseId",
									"type": "String"
								},
								"sink": {
									"name": "venue_link",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_id",
									"type": "String"
								},
								"sink": {
									"name": "city",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_link",
									"type": "String"
								},
								"sink": {
									"name": "abbreviation",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_locationName",
									"type": "String"
								},
								"sink": {
									"name": "team_name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_name",
									"type": "String"
								},
								"sink": {
									"name": "location_name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_officialSiteUrl",
									"type": "String"
								},
								"sink": {
									"name": "firstYearOfPlay",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "teams_shortName",
									"type": "String"
								},
								"sink": {
									"name": "division_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "teams_teamName",
									"type": "String"
								},
								"sink": {
									"name": "division_name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_conference_id",
									"type": "String"
								},
								"sink": {
									"name": "name_short",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_conference_link",
									"type": "String"
								},
								"sink": {
									"name": "conference_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "teams_conference_name",
									"type": "String"
								},
								"sink": {
									"name": "fanchise_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "teams_division_abbreviation",
									"type": "String"
								},
								"sink": {
									"name": "officialSiteUrl",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "teams_division_id",
									"type": "String"
								},
								"sink": {
									"name": "active",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Generic_Csv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "stage_Teams",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Truncate staging tables",
				"description": "Truncates tables in stagin schema",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "NHLDW",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "TRUNCATE TABLE stage.Teams\nTRUNCATE TABLE stage.Standings"
						}
					]
				}
			}
		],
		"annotations": []
	}
}